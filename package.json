{
  "name": "vectormap",
  "version": "0.0.1",
  "description": "Basic vector tile rendering with Canvas 2D",
  "main": "dist/vectormap.bundle.js",
  "files": [
    "dist"
  ],
  "directories": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-module && npm run build-examples",
    "build-module": "rollup -c build/rollup.config.js",
    "build-examples": "npm run rollup-examples && npm run copy-css",
    "rollup-examples": "cd examples && rollup -c",
    "copy-css": "echo examples/*/ | xargs -n 1 cp dist/vectormap.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjhembd/vectormap.git"
  },
  "keywords": [
    "vector",
    "tile",
    "map",
    "canvas"
  ],
  "author": "Jeshurun Hembd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jjhembd/vectormap/issues"
  },
  "homepage": "https://github.com/jjhembd/vectormap#readme",
  "devDependencies": {
    "http-server": "^0.11.1",
    "rollup": "^1.11.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.2.3"
  },
  "dependencies": {
    "csscolorparser": "^1.0.3",
    "d3-geo": "^1.11.3",
    "pbf": "github:jjhembd/pbf",
    "topojson-client": "^3.0.0",
    "vector-tile-js": "github:jjhembd/vector-tile-js"
  }
}
